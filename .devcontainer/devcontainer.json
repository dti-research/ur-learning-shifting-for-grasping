{
    "name": "dtiresearch/ur-lsfg",
    "remoteUser": "root",
    "context": "../",
    "dockerFile": "../docker/Dockerfile",
    "extensions": [
        "ms-python.python",
        "njpwerner.autodocstring"
    ],
    "runArgs": [
        // This will allow you to use a ptrace-based debugger like C++, Go, and Rust.
        "--cap-add=SYS_PTRACE",
        "--security-opt", "seccomp=unconfined",
    ],
}